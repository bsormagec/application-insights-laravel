{
    "name": "sormagec/application-insights-laravel",
    "description": "Microsoft Azure Application Insights for Laravel 10+ - A fully maintained package for pushing telemetry to Azure Application Insights",
    "homepage": "https://github.com/bsormagec/application-insights-laravel",
    "license": "MIT",
    "keywords": [
        "Laravel",
        "Microsoft",
        "Azure",
        "Application Insights",
        "monitoring",
        "logging",
        "telemetry",
        "APM",
        "performance"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Burak Sormagec",
            "email": "bsormagec@gmail.com",
            "homepage": "https://github.com/bsormagec"
        },
        {
            "name": "Sahib",
            "email": "sahib.zer@gmail.com",
            "role": "Original Author"
        }
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=10.0",
        "guzzlehttp/guzzle": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Sormagec\\AppInsightsLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sormagec\\AppInsightsLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sormagec\\AppInsightsLaravel\\Providers\\AppInsightsServiceProvider"
            ],
            "aliases": {
                "AIClient": "Sormagec\\AppInsightsLaravel\\Facades\\AppInsightsClientFacade",
                "AIServer": "Sormagec\\AppInsightsLaravel\\Facades\\AppInsightsServerFacade",
                "AIQueue": "Sormagec\\AppInsightsLaravel\\Facades\\AppInsightsQueueFacade"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "support": {
        "issues": "https://github.com/bsormagec/application-insights-laravel/issues",
        "source": "https://github.com/bsormagec/application-insights-laravel"
    }
}